{
  "module": "./esm/mod.js",
  "name": "tsignal_ts",
  "version": "0.1.1",
  "description": "a topological order respecting signals library inspired by SolidJS",
  "author": "Omar Azmi",
  "license": "Lulz plz don't steal yet",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omar-azmi/tsignal_ts.git"
  },
  "bugs": {
    "url": "https://github.com/omar-azmi/tsignal_ts/issues"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "typescript": "^5.0.0",
    "typedoc": "^0.24.8",
    "esbuild": "^0.17.19"
  },
  "scripts": {
    "build-docs": "npx typedoc",
    "build-dist": "npm run build-esm && npm run build-esm-minify && npm run build-iife && npm run build-iife-minify",
    "build-esm": "npx esbuild \"./src/mod.ts\" --bundle --format=esm --outfile=\"./dist/tsignal_ts.esm.js\"",
    "build-esm-minify": "npx esbuild \"./src/mod.ts\" --bundle --minify --format=esm --outfile=\"./dist/tsignal_ts.esm.min.js\"",
    "build-iife": "npx esbuild \"./src/mod.ts\" --bundle --format=iife --outfile=\"./dist/tsignal_ts.iife.js\"",
    "build-iife-minify": "npx esbuild \"./src/mod.ts\" --bundle --minify --format=iife --outfile=\"./dist/tsignal_ts.iife.min.js\""
  },
  "exports": {
    ".": {
      "import": "./esm/mod.js"
    },
    "./context": {
      "import": "./esm/context.js"
    },
    "./signal": {
      "import": "./esm/signal.js"
    },
    "./mapped_signal": {
      "import": "./esm/mapped_signal.js"
    },
    "./funcdefs": {
      "import": "./esm/funcdefs.js"
    },
    "./typedefs": {
      "import": "./esm/typedefs.js"
    }
  }
}